#include <iostream>

#define int long long
#define endl "\n"

using namespace std;

void solve()
{
    int xa, xb, Xa, Xb;
    cin>>xa>>xb>>Xa>>Xb;
    cout<<(Xa/xa) + (Xb/xb)<<endl;
}

int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
	return 0;
}
