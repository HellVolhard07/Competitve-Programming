//#include <math.h>

#define int long long
#define endl "\n"

using namespace std;

void solve()
{
  int ts,js, ans=0;
	cin >> ts;
	if(ts%2!=0)
	{
	    ans=ts/2;
	}
	else 
	{
	    while(ts%2==0)
	    {
	        ts = ts/2;
	    }
	    ans = ts/2;
	}
	cout<<ans<<endl;
}

int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
	return 0;
}
