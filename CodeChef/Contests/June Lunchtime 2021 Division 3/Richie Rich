#include <iostream>

#define int long long
#define endl "\n"

using namespace std;

void solve()
{
    int a, b, x;
    cin>>a>>b>>x;
    int num = b - a;
    if(num % x == 0)
    {
        cout<<num / x<<endl;;
    }
    else
    {
        cout<<(num / x) + 1<<endl;
    }
}

int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
	return 0;
}
