#include <iostream>

#define int long long
#define endl "\n"

using namespace std;

void solve()
{
    int x, a, b;
    cin>>x>>a>>b;
    cout<<(a+((100-x)*b))*10<<endl;
}

int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        solve();
    }
	return 0;
}
