#include <iostream>

#define int long long
#define endl "\n"

using namespace std;

void solve()
{
  int n;
	cin>> n;
	int sum = 0;
	for(int i= 0; i<n ; i++)
	{
	    int x;
	    cin>>x;
	    sum += x;
	}
	cout<<sum;
}

int32_t main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    solve();
	return 0;
}
